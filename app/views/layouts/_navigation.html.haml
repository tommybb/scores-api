%ul.nav.navbar-nav.pull-left
  %li= link_to 'Results', results_path
  %li= link_to 'Users', users_path
  %li= link_to 'Competitions', competitions_path
%ul.nav.navbar-nav.pull-right
  %li.navbar-text
    - if user_signed_in?
      Logged in as: #{current_user.name + ' ' + current_user.last_name}
      %li= link_to 'Logout', destroy_user_session_path, method: :delete
    - else
      %li
      %li= link_to 'SignUp', new_registration_path(resource_name), class: 'glyphicon glyphicon-user'
      %li= link_to 'Login', new_session_path(resource_name), class: 'glyphicon glyphicon-log-in'
